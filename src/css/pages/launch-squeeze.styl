/*Eliminates padding, centers the thumbnail */

body, html {
padding: 0;
margin: 0;
text-align: center;
}

/* Styles the close link, adds the slide down transition */
a.lightbox-close1
  left: 44.5%;
  @media screen and (max-width:500px)
    left: auto

a.lightbox-close2 {
  top: 10px;right: 10px;
}
a.lightbox-close
  display: block;
  width:50px;
  height:50px;
  box-sizing: border-box;
  margin-left: 200px;
  color: gray;
  text-decoration: none;
  position: absolute;
  top: -9px
  -webkit-transition: .5s ease-in-out;
  -moz-transition: .5s ease-in-out;
  -o-transition: .5s ease-in-out;
  transition: .5s ease-in-out;
  @media screen and (max-width:767px)
    right: 0px
    bottom: 105%;
    top: auto

/* Provides part of the "X" to eliminate an image from the close link */

a.lightbox-close:before {
content: "";
display: block;
height: 46px;
width: 3px;
background: #6f6c6c;
position: absolute;
left: 26px;
top:10px;
-webkit-transform:rotate(45deg);
-moz-transform:rotate(45deg);
-o-transform:rotate(45deg);
transform:rotate(45deg);
}

/* Provides part of the "X" to eliminate an image from the close link */

a.lightbox-close:after {
content: "";
display: block;
height: 46px;
width: 3px;
background: #6f6c6c;
position: absolute;
left: 26px;
top:10px;
-webkit-transform:rotate(-45deg);
-moz-transform:rotate(-45deg);
-o-transform:rotate(-45deg);
transform:rotate(-45deg);
}

/* Styles the thumbnail */

a.lightbox img {
height: 150px;
border: 3px solid white;
box-shadow: 0px 0px 8px rgba(0,0,0,.3);
margin: 94px 20px 20px 20px;
}

/* Styles the lightbox, removes it from sight and adds the fade-in transition */

.lightbox-target .trailer-container{
display: none

}

.lightbox-target2 {
position: fixed;
top: -100%;
width: 100%;

width: 100%;
opacity: 0;
-webkit-transition: opacity .5s ease-in-out;
-moz-transition: opacity .5s ease-in-out;
-o-transition: opacity .5s ease-in-out;
transition: opacity .5s ease-in-out;
overflow: hidden;
z-index: 9999;
}

/* Uses the :target pseudo-class to perform the animations upon clicking the .lightbox-target anchor */

.lightbox-target:target .trailer-container {
display: block
// z-index: 9999;
}
.lightbox-target2:target  {
opacity: 1;
top: 0;
bottom: 0;
// z-index: 9999;
}
.lightbox-target:target img, .lightbox-target:target .name, .lightbox-target:target .subtitle,
.lightbox-target:target .subtitle2 {
display:none
}


